   <p v-else>Success! Check your email for a reset link.</p>\n    </div>\n  </div>\n</template>\n\n<script>\nimport { auth } from '@/firebase'\n\nexport default {\n  data() {\n    return {\n      email: '',\n      showSuccess: false,\n      errorMsg: ''\n    }\n  },\n  methods: {\n    async resetPassword() {\n      this.errorMsg = ''\n\n      try {\n        await auth.sendPasswordResetEmail(this.email)\n        this.showSuccess = true\n      } catch (err) {\n        this.errorMsg = err.message\n      }\n    }\n  }\n}\n</script>\n"],"sourceRoot":"src/components"}]}